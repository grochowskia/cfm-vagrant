    router-id 2.2.2.5;
    autonomous-system 64512;
}
protocols {
    bgp {
        group CLOS {
            type external;
            family inet {
                unicast;
            }
            export export-bgp;
            local-as 64503;
            neighbor 10.0.6.1 {
                peer-as 64601;
            }
            neighbor 10.0.9.1 {
                peer-as 64602;
            }
        }
    }
    lldp {
        interface all;
    }
}
policy-options {
    policy-statement MAINTENANCE-MODE {
        term term1 {
            from {
                family evpn;
                nlri-route-type [ 2 1 3 4 5 ];
            }
            then {
                community add COM-MAINTENANCE;
                accept;
            }
        }
    }
    policy-statement MAINTENANCE-MODE-underlay {
        then {
            as-path-prepend "9999 9999 9999";
            accept;
        }
    }
    policy-statement REJECT-MAINTENANCE-MODE {
        term term1 {
            from {
                family evpn;
                nlri-route-type [ 2 1 3 4 5 ];
            }
            then {
                community add COM-MAINTENANCE;
                reject;
            }
        }
    }
    policy-statement export-bgp {
        term 1 {
            from {
                protocol [ direct local ];
                interface [ lo0.0 irb.0 ];
            }
            then accept;
        }
        term 2 {
            from protocol bgp;
            then accept;
        }
        term 3 {
            then reject;
        }
    }
    community COM-MAINTENANCE members 9999:9999;
}
vlans {
    default {
        vlan-id 1;
        l3-interface irb.0;
    }
}

